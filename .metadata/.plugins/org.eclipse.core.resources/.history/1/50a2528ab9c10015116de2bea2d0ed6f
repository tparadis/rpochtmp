function addSsCat(sscat, tab) {
	tab.push(sscat);
	
	var numberOfListItems = tab.length;   			   
    var i = 0;
    while (i < numberOfListItems)
    {
    	$("tbody").append("<tr><td>"+tab[i]+"</td><td>test</td><td>test</td></tr>");
    	i++;
    }
}